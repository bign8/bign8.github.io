<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<!-- Posts -->{% for post in site.posts %}
	<url><loc>http://bign8.info{{ post.url | remove: 'index.html' }}</loc></url>{% endfor %}
	<!-- Pages -->{% for page in site.pages %}{% if page.layout != nil %}
	<url><loc>http://bign8.info{{ page.url | remove: 'index.html' }}</loc></url>{% endif %}{% endfor %}
</urlset>
